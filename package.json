    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@types/eslint": "^7.28.2",
    "@types/eslint-utils": "^3.0.0",
    "@types/estree": "^0.0.50",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@types/jquery": "^3.5.8",
