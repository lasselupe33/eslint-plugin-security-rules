{
  "name": "eslint-plugin-security-rules",
  "version": "0.0.2",
  "description": "",
  "main": "./lib/index.js",
  "files": [
    "lib/**"
  ],
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "scripts": {
    "build": "rm -rf ./lib && tsc --project ./tsconfig.build.json",
    "watch": "yarn build --watch",
    "prepublishOnly": "yarn build && cp -r ./docs ./lib/docs",
    "eslint:files": "eslint --max-warnings 0",
    "eslint:plugin": "yarn eslint:files 'src/**/*.{ts,tsx}' --ignore-pattern '**/tests/**/*.ts'",
    "eslint:tests": "yarn eslint:files 'src/**/tests/**/*.{ts,tsx}'",
    "eslint:dummy": "yarn eslint:files 'dummy/**/*.{ts,tsx}'",
    "prepare": "husky install",
    "test": "jest --cache --config=.jestrc.json",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:lasselupe33/eslint-plugin-security-rules.git"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@swc-node/jest": "^1.4.3",
    "@types/dompurify": "^2.3.3",
    "@types/eslint": "^8.4.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.0",
    "@types/mysql": "^2.15.20",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "core-js": "^3.21.0",
    "dompurify": "^2.3.6",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-comment-length": "^0.5.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security-rules": "link:./",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.3",
    "mysql": "^2.18.1",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "dependencies": {
    "@npmcli/arborist": "^4.3.0",
    "@npmcli/metavuln-calculator": "^2.0.0",
    "@typescript-eslint/utils": "^5.10.2"
  }
}
