{
  "name": "eslint-plugin-security-rules",
  "version": "0.0.2",
  "description": "",
  "main": "./lib/index.js",
  "files": [
    "lib/**"
  ],
  "private": true,
  "workspaces": {
    "packages": [
      "./dummy"
    ]
  },
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintplugin",
    "security"
  ],
  "scripts": {
    "build": "rm -rf ./lib && tsc --project ./tsconfig.build.json",
    "watch": "yarn build --watch",
    "prepublishOnly": "yarn build && cp -r ./docs ./lib/docs",
    "eslint:files": "eslint",
    "eslint:plugin": "yarn eslint:files 'src/**/*.{ts,tsx}' --ignore-pattern '**/tests/**/*.ts' --ignore-pattern '**/tests/**/*.tsx'",
    "eslint:tests": "yarn eslint:files 'src/**/tests/**/*.{ts,tsx}'",
    "eslint:dummy": "yarn eslint:files 'dummy/**/*.{ts,tsx}'",
    "prepare": "husky install",
    "test": "jest --cache --config=.jestrc.json",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:lasselupe33/eslint-plugin-security-rules.git"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@swc-node/jest": "^1.4.3",
    "@types/eslint": "^8.4.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@typescript-eslint/scope-manager": "^5.12.1",
    "core-js": "^3.21.1",
    "enhanced-resolve": "^5.9.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-comment-length": "^0.5.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security-rules": "link:./",
    "fs-extra": "^10.0.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.12.1",
    "semver": "^7.3.5",
    "sync-fetch": "https://github.com/lasselupe33/sync-fetch.git"
  }
}
