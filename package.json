  "scripts": {
    "build": "rm -rf ./lib && tsc --project ./tsconfig.build.json",
    "watch": "yarn build --watch",
    "prepublishOnly": "yarn build && cp -r ./docs ./lib/docs",
    "eslint:files": "eslint",
    "eslint:plugin": "yarn eslint:files 'src/**/*.{ts,tsx}'",
    "eslint:dummy": "yarn eslint:files 'dummy/**/*.{ts,tsx}'",
    "prepare": "husky install",
    "test": "jest --cache --config=.jestrc.json",
    "perf": "node --inspect -r ts-node/register ./perf/performanceTest.ts 1000 10000 ./perf/generated-large-400/entrypoint.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:lasselupe33/eslint-plugin-security-rules.git"
  },
